<VBox alignment="CENTER" prefHeight="440.0" prefWidth="320.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tictactoefx.TTTController">
    <padding>
