   <HBox prefHeight="31.0" prefWidth="280.0">
      <children>
      
          <Label fx:id="welcomeText" alignment="CENTER" prefHeight="30.0" prefWidth="277.0" text="Welcome to TicTacToe Game!" textAlignment="CENTER">
